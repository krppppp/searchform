<p id="notice"><%= notice %></p>

<h1>Serieses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @records.each do |seriese| %>
      <tr>
        <td><%= link_to 'Show', seriese %></td>
        <td><%= link_to 'Edit', edit_record_path(seriese) %></td>
        <td><%= link_to 'Destroy', seriese, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Record', new_record_path %>
