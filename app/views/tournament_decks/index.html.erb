<p id="notice"><%= notice %></p>

<h1>Tournament Decks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tournament_decks.each do |tournament_deck| %>
      <tr>
        <td><%= link_to 'Show', tournament_deck %></td>
        <td><%= link_to 'Edit', edit_tournament_deck_path(tournament_deck) %></td>
        <td><%= link_to 'Destroy', tournament_deck, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tournament Deck', new_tournament_deck_path %>
